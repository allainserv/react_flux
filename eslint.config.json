{
    "ecmaFeatures": {
        "jsx": true
    },
    "env":{
        "browser": true,
        "node": true,
        "jquery": true
    },
    "rules": {
        
        "no-trailing-spaces": 0,
        "eol-last":0,
        "no-unused-vars": 0,
        "no-underscore-dangle":0,
        "no-alert": 0,
        "no-alone-blocks":0
    },
    "globals":{
        "jQuery": true,
        "$": true
    }

}